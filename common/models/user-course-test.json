{
  "name": "userCourseTest",
  "description": "Egzamin uszestnika|Egzaminy uszestnik√≥w",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "courseTestId": {
      "type": "number",
      "required": true,
      "title": "Egzamin szkolenia"
    },
    "userId": {
      "type": "number",
      "required": true,
      "title": "Uczestnik"
    },
    "completeDt": {
      "type": "date",
      "title": "Data wykonania testu"
    },
    "checkDt": {
      "type": "date",
      "title": "Data sprawdzenia testu"
    },
    "score": {
      "type": "number",
      "title": "Wynik (punkty)"
    },
    "scorePercent": {
      "type": "number",
      "title": "Wynik (%)"
    },
    "passed": {
      "type": "boolean",
      "title": "Egzamin zaliczoy (Tak/Nie)"
    }
  },
  "validations": [],
  "relations": {
    "userAnswers": {
      "type": "hasMany",
      "model": "userAnswer",
      "foreignKey": ""
    },
    "courseTest": {
      "type": "belongsTo",
      "model": "courseTest",
      "foreignKey": "courseTestId"
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {
    "evaluate": {
      "accepts": [
        {
          "arg": "userTestId",
          "type": "array",
          "required": true,
          "description": ""
        }
      ],
      "returns": [
        {
          "arg": "userTest",
          "type": "array",
          "root": true,
          "description": "Evaluated userTest"
        }
      ],
      "description": "Evaluats user test, set score an so on.",
      "http": [
        {
          "path": "/evaluate",
          "verb": "get"
        }
      ]
    }
  }
}
