{
  "name": "course",
  "description": "Szkolenie|Szkolenia",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "SoftDelete": true,
    "TimeStamp": true
  },
  "title": "Kursy",
  "mssql": {
    "schema": "dbo",
    "table": "course"
  },
  "scope": {
    "include": []
  },
  "properties": {
    "name": {
      "type": "string",
      "required": false,
      "title": "Nazwa"
    },
    "startDt": {
      "type": "date",
      "required": true,
      "title": "Data rozpoczęcia"
    },
    "endDt": {
      "type": "date",
      "required": true,
      "title": "Data zakńczenia"
    },
    "logoMediaId": {
      "type": "number",
      "required": false,
      "title": "Zjęcie"
    },
    "purpose": {
      "type": "string",
      "title": "Cel szkolenia",
      "mssql": {
        "columnName": "purpose",
        "dataType": "nvarchar",
        "dataLength": "max",
        "dataPrecision": null,
        "dataScale": null,
        "nullable": "YES"
      }
    },
    "agenda": {
      "type": "string",
      "title": "Program szkolenia",
      "mssql": {
        "columnName": "agenda",
        "dataType": "nvarchar",
        "dataLength": "max",
        "dataPrecision": null,
        "dataScale": null,
        "nullable": "YES"
      }
    },
    "recipient": {
      "type": "string",
      "title": "Dla kogo",
      "mssql": {
        "columnName": "recipient",
        "dataType": "nvarchar",
        "dataLength": "max",
        "dataPrecision": null,
        "dataScale": null,
        "nullable": "YES"
      }
    },
    "desc": {
      "type": "string",
      "title": "Opis",
      "mssql": {
        "columnName": "desc",
        "dataType": "nvarchar",
        "dataLength": "max",
        "dataPrecision": null,
        "dataScale": null,
        "nullable": "YES"
      }
    },
    "free": {
      "type": "boolean",
      "title": "Bezpłatne"
    },
    "goelocLat": {
      "type": "number",
      "title": "Szerokość geograficzna"
    },
    "goelocLong": {
      "type": "number",
      "title": "Długość geograficzna"
    }
  },
  "validations": [],
  "relations": {
    "users": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "courseId",
      "through": "userCourse",
      "keyThrough": "userId"
    },
    "trainers": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "courseId",
      "through": "trainerCourse",
      "keyThrough": "userId"
    },
    "files": {
      "type": "hasMany",
      "model": "mediaLink",
      "polymorphic": {
        "foreignKey": "refId",
        "discriminator": "refType"
      }
    },
    "logoMedia": {
      "type": "belongsTo",
      "model": "media",
      "foreignKey": "logoMediaId"
    },
    "courseTests": {
      "type": "hasMany",
      "model": "courseTest",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "administrator",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
